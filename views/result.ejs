        <span class="leaderboard__name" id="positive-emotions"></span>
        <span class="leaderboard__value">
          <%= pos %>
            <% if (pos < 5) { %>
              <span class="emoji">&#128532;</span> <!-- sad emoji -->
              <% } else { %>
                <span class="emoji">&#128522;</span> <!-- happy emoji -->
                <% } %>
